{
  "version": 2,
  "build": {
    "env": {
      "API_URL": "@slackin-api-url",
      "RECAPTCHA_SITE_KEY": "@slackin-recaptcha-site-key",
      "RECAPTCHA_SECRET_KEY": "@slackin-recaptcha-secret-key",
